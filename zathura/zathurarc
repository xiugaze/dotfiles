map u scroll half-up
map d scroll half-down
map D toggle_page_mode
map r reload
map R rotate
map i recolor
map p print
map g goto top
set default-bg rgba(46,52,64,0.96)
set recolor true
set recolor-lightcolor rgba(46,52,64,0.96)
set recolor-darkcolor rgba(216, 222, 233, 1)

set selection-clipboard clipboard
set font "JetBrains Mono normal 12"
